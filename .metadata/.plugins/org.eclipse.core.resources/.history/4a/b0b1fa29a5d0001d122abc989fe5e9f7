import java.util.Scanner;
 
public class Main {
 
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        
        Double a = input.nextDouble();
        Double b = input.nextDouble();
        double media = (a + b) /2;
        System.out.printf("MEDIA = %.5f%n"+media);
    
        input.close();  
    }
 
}