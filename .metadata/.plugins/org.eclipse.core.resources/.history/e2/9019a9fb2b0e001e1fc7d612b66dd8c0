public class TestaCaracteres{

    public static void main(String[] args){

        char letra = 'a';
        System.out.println(letra);

        char valor = 65;           // Compila!
        System.out.println(valor);

        valor = valor + 1;         // NÃ£o compila!
        System.out.println(valor);
    }
}

		
	}

}
