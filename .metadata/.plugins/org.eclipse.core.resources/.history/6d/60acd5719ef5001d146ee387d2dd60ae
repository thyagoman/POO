import java.util.Scanner;


public class Main{
    public static void main (String[]args){
        Scanner input = new Scanner(System.in);
        
     
     
        
        
        int n = input.nextInt();
        double x,y,divisao;
        
        
        
        for (int i = 0; i < n; i++) {
        	x = input.nextDouble();
        	y = input.nextDouble();
        	
        	if (x < 0 && y >= 0) {
        		System.out.println("divisao impossivel");
        	}else if (x >=0 && y <= 0) {
        		divisao = x / y;
        		System.out.printf("%.1f%n",divisao);
        	}else {
        		divisao = y / x;
        		System.out.println(divisao);
        	}
        	
        }	
    }
    
}