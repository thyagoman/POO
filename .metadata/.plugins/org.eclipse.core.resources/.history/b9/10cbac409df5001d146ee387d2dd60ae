import java.util.Scanner;


public class Main{
    public static void main (String[]args){
        Scanner input = new Scanner(System.in);
        
     
     
        
        
        int n = input.nextInt();
        int x,y,divisao;
        
        
        
        for (int i = 0; i < n; i++) {
        	x = input.nextInt();
        	y = input.nextInt();
        	
        	if (x < 0 && y >= 0) {
        		System.out.println("divisao impossivel");
        	}else if (x >=0 && y <= 0) {
        		divisao = x / y;
        		System.out.println(divisao);
        	}else if ((y >= 0 && x >=0)|| x==0);
        		divisao = y / x;
        		System.out.println((double)divisao);
        	
        }	
    }
    
}